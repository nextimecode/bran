{
  "name": "bran",
  "version": "1.13.3",
  "description": "Backend - Bran, inspirado em Bran Stark, reflete inteligência e visão estratégica. Este projeto é o núcleo lógico do sistema, responsável por processar, organizar e prover os dados de forma eficiente e segura para suportar as operações de todas as interfaces conectadas.",
  "author": "Pedro Duarte",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "prisma generate && nest start --watch",
    "clean": "rm -rf .turbo .next .vercel dist node_modules pnpm-lock.yaml playwright-report test-results",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "e2e": "vitest run --config ./vitest.config.e2e.ts",
    "e2e:watch": "vitest --config ./vitest.config.e2e.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.749.0",
    "@nestjs/common": "^11.0.9",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.9",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.9",
    "@nestjs/swagger": "^11.0.3",
    "@prisma/client": "^6.3.1",
    "@rollup/rollup-darwin-arm64": "^4.34.7",
    "@types/aws-sdk": "^2.7.4",
    "bcryptjs": "^3.0.0",
    "dayjs": "^1.11.13",
    "ioredis": "^5.5.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.5.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.9",
    "@rocketseat/eslint-config": "^2.2.2",
    "@swc/core": "^1.10.16",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^3.0.5",
    "dotenv": "^16.4.7",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^6.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  }
}
